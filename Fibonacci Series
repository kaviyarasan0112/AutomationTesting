package week1Day1;

public class FibonacciSeries {
	
	//Range 8
	//0,1,1,2,3,5,8,13,
	
	public void fibonacci(){
		int n1=0;
		int n2=1;
		int n3;
		System.out.print(n1+",");
		System.out.print(n2+",");
		
		for(int i=1;i<=8;i++) {
			n3=n1+n2;
			n1=n2;
			n2=n3;
			System.out.print(n3+",");
		}
	}
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		FibonacciSeries fs=new FibonacciSeries();
		fs.fibonacci();
		

	}

}
