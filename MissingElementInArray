package interviewPrograms;

public class MissingElementInArray {

	public static void main(String[] args) {
		

		int[] arr = new int[]{4,5,7,8,9,10};
		
	
		
        int missing=0;
        for(int i=0;i<arr.length;i++)
        {  

            int x = arr[i++];
            int y = arr[i] +1;

          if(x != y )
          {
              missing = y;
              break;
          }
        }

        System.out.println(missing);
		
		
	}

}
